<h1 class="site-title">{{settings.site.title}}</h1>

{{#if (isEnabled "search")}}
  {{>features/search}}
{{/if}}

<nav class="categories">
    {{#each categories as |category|}}
        <a class="categories-item category-link" href="{{category.permalink}}">
            {{category.name}}
        </a>
    {{/each}}
</nav>
